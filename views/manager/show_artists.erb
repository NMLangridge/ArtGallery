<p>First Name: <%= @artist.first_name %></p>
<p>Last Name: <%= @artist.last_name %></p>

<% for exhibit in @exhibits %>
  <p><%= "Title:" %> <%= exhibit.title %></p>
<% end %>

<form method="post" action="/manager/artist/<%= @artist.id %>/edit">

  <input type="submit" value="Edit Artist"/>

</form>

<form method="post" action="/manager/artist/<%= @artist.id %>/delete">

  <input type="submit" value="Delete Artist"/>

</form>
